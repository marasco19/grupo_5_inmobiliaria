<!DOCTYPE html>
<html lang="en">
    <%- include("./partials/heads/headDef") %>
    <div id="container-main">
        <%- include("./partials/header") %> 
    <main>
        <%= console.log(propiedad) %>
        <section class="caracteristicas">
            <div class="container">
                <div id="carouselExampleControls" class="carousel slide w-70" data-ride="carousel">
                    <div class="carousel-inner">
                      <div class="carousel-item active">
                        <img class="d-block w-100" src="/images/propiedades/<%=propiedad.fotos1%>" alt="First slide">
                      </div>
                      <div class="carousel-item">
                        <img class="d-block w-100" src="/images/propiedades/<%=propiedad.fotos2%>" alt="Second slide">
                      </div>
                      <div class="carousel-item">
                        <img class="d-block w-100" src="/images/propiedades/<%=propiedad.fotos3%>" alt="Third slide">
                      </div>
                      <div class="carousel-item">
                        <img class="d-block w-100" src="/images/propiedades/<%=propiedad.fotos4%>" alt="Third slide">
                      </div>
                      <div class="carousel-item">
                        <img class="d-block w-100" src="/images/propiedades/<%=propiedad.fotos5%>" alt="Third slide">
                      </div>
                      <div class="carousel-item">
                        <img class="d-block w-100" src="/images/propiedades/<%=propiedad.fotos6%>" alt="Third slide">
                      </div>
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                      <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                      <span class="carousel-control-next-icon" aria-hidden="true"></span>
                      <span class="sr-only">Next</span>
                    </a>
                  </div>
                <div class="main-description">
                        <div class="top">
                            <div class="content-dir">
                                <br>
                                <span class="precio">USD <%= propiedad.precio %> </span>
                                <h1><%= propiedad.direccion %></h1>
                                <h2><%= propiedad.barrio %></h2>
                            </div>

                        </div>
                        <div class="content-description"> 
                            <p><%= propiedad.descripciongral %></p>
                        </div>
                            <span class="superficie"><i class="icon-19"></i>Superficie total: <%= propiedad.suptotal %> m<sup>2</sup></span>
                        <div class="group-list">
                            <div>
                                    <h2><strong> Aprox., Medidas Generales y Datos Técnicos</strong></h2>                       
                                    <ul>
                                        <li>Sup. Cubierta (m<sup>2</sup>): <strong><%= propiedad.supcubierta %></strong></li>
                                        <li>Sup. Semicubierta (m<sup>2</sup>): <strong><%= propiedad.supdescubierta %></strong></li>
                                    </ul>
                            </div>
                            <div>
                                <h2><strong>Descripción del Inmueble</strong></h2>
                                <ul>
                                    <li>Ambientes: <strong><%= propiedad.ambientes %></strong></li>
                                    <li>Cantidad de Baños: <strong><%= propiedad.banios %></strong></li>
                                    <li>Tipo de Cocina: <strong><%= propiedad.tipococina %></strong></li>

                                </ul>
                            </div>
                            <div>
                                <h2><strong>Descripción Complementaria</strong></h2>
                                <ul>
                                    <li>Expensas: <strong>$ <%= propiedad.expensas %></strong></li>
                                        <li>ABL: <strong>$<%= propiedad.abl %></strong></li>
                                </ul>
                            </div>
                            <div>
                                <h2><strong>Características del Edificio</h3></strong></h2>                        
                                <ul>
                                    <% if (propiedad.parrilla) { %>
                                        <li>Parrilla</li>
                                    <% } %>
                                    <% if (propiedad.piscina) { %>
                                        <li>Piscina</li>
                                    <% } %>
                                    <% if (propiedad.quincho) { %>
                                        <li>Quincho</li>
                                    <% } %>
                                    <% if (propiedad.gimnasio) { %>
                                        <li>Gimnasio</li>
                                    <% } %>
                                    <% if (propiedad.salonfiestas) { %>
                                        <li>Salón de fiestas</li>
                                    <% } %>
                                    <% if (propiedad.seguridad) { %>
                                        <li>Seguridad 24hs</li>
                                    <% } %>
                                    <% if (propiedad.grupoelec) { %>
                                        <li>Grupo electrogeno</li>
                                    <% } %>

                                </ul>
                            </div>    
                            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3284.5273566406204!2d<%=propiedad.latitud%>!3d<%=propiedad.longitud%>!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x0!2zMzTCsDM1JzI3LjAiUyA1OMKwMjQnMDUuMSJX!5e0!3m2!1ses!2sar!4v1622720034163!5m2!1ses!2sar" width="800" height="600" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
                            <form method="GET" action="/admin/addFavorito/<%=propiedad.id %>">
                                <button class="boton-favorito">Agregar a favoritos</button>
                            </form>
                            <% }%>
                                
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="formulario-contacto">
                <div class="wrapper">
                    <div class="formulario">
                        <h4>Sucursal Belgrano</h4>
                        <div class="IsWhatsapp">
                            <span class="tel"><a href="tel:'(011) 4666-6666''" style="color:#333333"><i class="fa fa-phone"></i>(011) 4666-6666</a></span>
                            <a href="#" target="_blank"" style="color:black">
                                <span class="tel"><i class="fa fa-whatsapp"></i>1135715151</span>
                            </a>                   
                        </div>
                        <strong>Consultar:</strong>
                        <form class="contactar" action="/" method="POST" class="create-form">
                            <input type="text" name="name" placeholder="Nombre y Apellido" id="frm_name">
                            <input type="text" name="email" placeholder="E-mail" id="frm_email">
                            <input type="number" name="telefono" placeholder="Teléfono" id="frm_telefono">
                            <textarea name="comentario" id="frm_comentario" placeholder="Comentario"></textarea>
                            <span class="boton-enviar">Enviar</span>
                        </form>
                    </div>
                </div>
        </section>
    </main>    
    <%- include("./partials/footer") %> 
    </div>   
</body>
</html>